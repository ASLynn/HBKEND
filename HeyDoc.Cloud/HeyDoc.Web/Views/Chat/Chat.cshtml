@using HeyDoc.Web;

<div class="row">
    <div id="breadcrumb" class="col-xs-12">
        <a href="#" class="show-sidebar">
            <i class="fa fa-bars"></i>
        </a>
        <ol class="breadcrumb pull-left">
            <li><a href="#">User List</a></li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-lg-5">
        <div class="box">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-user-md"></i>
                    <span>Chat Room List</span>
                </div>
                <div class="box-icons">
                    <a class="expand-link">
                        <i class="fa fa-expand"></i>
                    </a>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content no-padding">
                <!-- Table -->
                <table id="chatRoomList" class="table table-bordered table-striped table-hover table-heading table-datatable">
                    <thead>
                        <tr>
                            <th>Rank</th>
                            <th>Doctor</th>
                            <th>Patient</th>
                            <th>Patient Package</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                    <tfoot>
                        <tr>
                            <th>Rank</th>
                            <th>Doctor</th>
                            <th>Patient</th>
                            <th>Patient Package</th>
                            <th></th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>


    <div class="col-lg-7">
        <div class="box">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-comments fa-fw"></i>
                    <span>Chat List</span>
                </div>
                <div class="box-icons">
                    <a class="expand-link">
                        <i class="fa fa-expand"></i>
                    </a>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content">
                <!-- Table -->
                <table id="chatList" class="table table-bordered table-striped table-hover table-heading table-datatable">
                    <thead>
                        <tr>
                            <th>Rank</th>
                            <th>Doctor</th>
                            <th>Patient</th>
                            <th>Patient Package</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                    <tfoot>
                        <tr>
                            <th>Rank</th>
                            <th>Doctor</th>
                            <th>Patient</th>
                            <th>Patient Package</th>
                            <th></th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
</div>

